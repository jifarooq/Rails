<h1> Track page for <%= @track.track_name %> </h1>

<%= link_to "Track's Album", album_url(@track.album_id) %>

<ul>
	<% @track.attributes.each do |attr| %>
		<li>
			<%= "#{attr.first.capitalize}: #{attr.last}"%><br>
		</li>
	<% end %>
</ul>

<%= link_to "Edit track", edit_track_url%>
<%= button_to "Annihilate track", track_url, method: :delete%>
<br>


<ul>
<li><%= link_to "Create track", new_album_track_url(@track.album_id) %></li>
<li><%= link_to "Create album", new_band_album_url(@track.band.id) %></li>
<li><%= link_to "Create band", new_band_url%></li>
</ul>